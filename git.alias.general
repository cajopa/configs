; include via alias.path

[alias]
	cob=checkout -b
	col=checkout -
	com=checkout master
	root=!pwd
	unstage = reset HEAD --
	discard = checkout --
	st = status
	last = log -1 HEAD
	this = name-rev --name-only HEAD
	pi = !git push -u origin $(git this)
	pp = !git pull && git push
	cdr = !cd $(git root)
	gdu = diff @{u}
	consh = config --global --includes --list
	dc = diff --cached
	cv = commit -v
